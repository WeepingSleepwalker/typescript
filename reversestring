function backwardString(value: string): string {
    // your code here
    let reversedString = ""
    for (let i = value.length-1; i >=0; i--){
      reversedString += value[i]
    }
    return reversedString
}

//or

function backwardString(value: string): string {
    // your code here
    return value.split('').reverse().join('');
}

//or

function backwardString(value: string): string {
    // your code here
    return Array.from(value).reduce((total, current) => current + total, '');
}
